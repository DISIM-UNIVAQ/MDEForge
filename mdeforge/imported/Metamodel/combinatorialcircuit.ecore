<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="circuit" nsURI="http://mde.disim.univaq.it/combinatorialcircuitMM"
    nsPrefix="it.univaq.disim.mde.combinatorialcircuitMM">
  <eClassifiers xsi:type="ecore:EClass" name="Combinatorialcircuit" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="start2Gates" ordered="false"
        upperBound="-1" eType="#//Start2Gate" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="gate2Sinks" ordered="false"
        upperBound="-1" eType="#//Gate2Sink" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="gate2Gates" ordered="false"
        upperBound="-1" eType="#//Gate2Gate" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="starts" ordered="false"
        upperBound="-1" eType="#//Start" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sinks" ordered="false"
        upperBound="-1" eType="#//Sink" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="gates" upperBound="-1"
        eType="#//Gate" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="And" eSuperTypes="#//Gate"/>
  <eClassifiers xsi:type="ecore:EClass" name="Gate" abstract="true" eSuperTypes="#//NamedElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="Or" eSuperTypes="#//Gate"/>
  <eClassifiers xsi:type="ecore:EClass" name="Not" eSuperTypes="#//Gate"/>
  <eClassifiers xsi:type="ecore:EClass" name="Start" eSuperTypes="#//NamedElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="Sink" eSuperTypes="#//NamedElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="Data" abstract="true" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBooleanObject"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Start2Gate" eSuperTypes="#//Data">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//Start"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//Gate"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Gate2Sink" eSuperTypes="#//Data">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//Gate"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//Sink"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Gate2Gate" eSuperTypes="#//Data">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//Gate"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//Gate"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Nand" eSuperTypes="#//Gate"/>
  <eClassifiers xsi:type="ecore:EClass" name="Nor" eSuperTypes="#//Gate"/>
  <eClassifiers xsi:type="ecore:EClass" name="Xor" eSuperTypes="#//Gate"/>
  <eClassifiers xsi:type="ecore:EClass" name="Xnor" eSuperTypes="#//Gate"/>
  <eClassifiers xsi:type="ecore:EClass" name="Forward" eSuperTypes="#//Gate">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="threshold" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NamedElement" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
