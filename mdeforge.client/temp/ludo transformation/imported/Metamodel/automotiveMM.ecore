  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Notification_manager" eSuperTypes="#//Named_Element #//Controller">
    <eStructuralFeatures xsi:type="ecore:EReference" name="content" ordered="false"
        upperBound="-1" eType="#//Notification" containment="true" eOpposite="#//Notification/collector"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sender" ordered="false"
        lowerBound="1" eType="#//Security_control" eOpposite="#//Security_control/notifier"/>
