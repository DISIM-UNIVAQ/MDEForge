  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Controller" abstract="true" eSuperTypes="#//Named_Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="manager" lowerBound="1"
        eType="#//Central_unit"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="actuators" lowerBound="1"
        eType="#//List"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="component1" lowerBound="1"
        upperBound="-1" eType="#//Automotive_system"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="List">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="content" upperBound="-1"
