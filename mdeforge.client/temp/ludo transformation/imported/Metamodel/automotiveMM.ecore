  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Lock_control" eSuperTypes="#//Named_Element #//Security_control">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="locked" ordered="false"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Sensor_control" eSuperTypes="#//Named_Element #//Controller">
