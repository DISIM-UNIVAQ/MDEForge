package mw_traceability {

       class TraceModel extends WModel {
               reference wovenModels[*] container subsets wovenModel : TraceModelRef;
       }

       class TraceModelRef extends WModelRef {
          --reference ownedElementRef[*] container : ElementRef oppositeOf modelRef;
       }


-------------------------------------------------------------------
--   class WLink { represents the basic link semantics }

   class TraceLink  extends WLink{
       attribute ruleName : String;
       reference sourceElements[*] ordered container subsets end : WLinkEnd;
       reference targetElements[*] ordered container subsets end : WLinkEnd;
   }

       class ElementRef extends WElementRef {
               reference modelRef: TraceModelRef oppositeOf ownedElementRef;
       }
       class TraceLinkEnd extends WLinkEnd {
       }

}
