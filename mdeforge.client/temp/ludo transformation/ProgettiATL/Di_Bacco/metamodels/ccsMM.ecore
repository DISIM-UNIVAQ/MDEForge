  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CCS" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="actionSet" upperBound="-1"
        eType="#//Action" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="processSet" lowerBound="1"
